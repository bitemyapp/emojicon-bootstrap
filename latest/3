    var pastTop = $(".pulldown").css("top")
    $('.pulldown-tab:not(".opened") span').live("click", function() {
      $(".pulldown").css("top",pastTop).delay(150).animate({top:0}, 750, "easeOutQuint")
      $(this).parent().addClass("opened")
    })
    $(".pulldown-tab.opened span").live("click", function() {
      $(".pulldown").css("top",0).delay(150).animate({top: pastTop}, 750, "easeOutQuint")
      $(this).parent().removeClass("opened")
