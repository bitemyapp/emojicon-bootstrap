  //<![CDATA[
    var clip;
    var active;
    var clicked;
    var copiedText;
    ZeroClipboard.setMoviePath( '/swfs/ZeroClipboard.swf' );
    function initZeroClipboard() {
      clip = new ZeroClipboard.Client();
      clip.setHandCursor( true );
    
      // clip.addEventListener('load', function (client) {
      //   console.log("Flash movie loaded and ready.");
      // });
    
      clip.addEventListener('complete', function (client, text) {
        // console.log("Copied text to clipboard: " + text );
        clicked = active;
        $(clicked).animate({right: "-106px"}, 200, "swing", function() {
          var t = setTimeout(function() {
            $(clicked).fadeOut(250).delay(750).hide().css({right: "-52px"}).fadeIn(150);
          }, 3000)
        })
        
        objId = $(clicked).parents(".emoticon-item").attr("id").split("-");
        $.ajax({
          type: 'POST',
          url: '/e/' + objId[1] + '/copy',
          data: ({id: objId[1], source: "site" })
        })
      });
    
      clip.addEventListener('onMouseDown', function(client) {
        clip.setText( $('.copy-button.active').siblings(".text-wrapper").children(".text").text() );
      });
      $('.copy-button').mouseover( function() {
        clip.setText( $(this).siblings(".text-wrapper").children(".text").text() );
        active = this;
        if (clip.div) {
          clip.receiveEvent('mouseout', null);
          clip.reposition(this);
        }
        else clip.glue(this);
        clip.receiveEvent('mouseover', null);
      } );
    }
    
    $(document).ready(function() {
      initZeroClipboard()
    })
  //]]>
</script>

